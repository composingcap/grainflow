<c74object name='grainflow.spatview~' category='ui'>

	<digest>Visualize Grainflow spatialization </digest>
	<description>Visualize Grainflow spatialization </description>


	<!--METADATA-->

	<metadatalist>
		<metadata name='author'>Christopher Poovey </metadata>
		<metadata name='tag'>ui</metadata>
	</metadatalist>


	<!--ARGUMENTS-->

	<objarglist>

	</objarglist>


	<!--MESSAGES-->

	<methodlist>

		<method name='anything'>
			<digest>Dictionary of grain visualization information generated by grainflow</digest>
			<description>Dictionary of grain visualization information generated by grainflow.spat.pan~ </description>
		</method>

	</methodlist>


	<!--ATTRIBUTES-->

	<attributelist>

		<attribute name='backgroundColor' get='1' set='1' type='float64' size='1' >
			<digest>the color of the background </digest>
			<description>the color of the background </description>
		</attribute>

		<attribute name='centerColor' get='1' set='1' type='float64' size='1' >
			<digest>Color of the center handle </digest>
			<description>Color of the center handle </description>
		</attribute>

		<attribute name='drawCenter' get='1' set='1' type='long' size='1' >
			<digest>Enables/Disables the center handle </digest>
			<description>Enables/Disables the center handle </description>
		</attribute>

		<attribute name='grainColor' get='1' set='1' type='float64' size='1' >
			<digest>The primary grain color </digest>
			<description>The primary grain color </description>
		</attribute>

		<attribute name='grainColor2' get='1' set='1' type='float64' size='1' >
			<digest>The secondary grain color </digest>
			<description>The secondary grain color </description>
		</attribute>

		<attribute name='grainScale' get='1' set='1' type='float64' size='1' >
			<digest>Size of the grains </digest>
			<description>Size of the grains </description>
		</attribute>

		<attribute name='speakerOnColor' get='1' set='1' type='float64' size='1' >
			<digest>speaker on color when sounding </digest>
			<description>speaker on color when sounding </description>
		</attribute>

		<attribute name='speakerPositions' get='1' set='1' type='float64' size='1' >
			<digest>Speaker positions in a list of XYZ coordinates </digest>
			<description>Speaker positions in a list of XYZ coordinates </description>
		</attribute>

		<attribute name='zoneColor' get='1' set='1' type='float64' size='1' >
			<digest>the color of the speaker zone </digest>
			<description>the color of the speaker zone </description>
		</attribute>

	</attributelist>


	<!--RELATED-->

	<seealsolist>
	</seealsolist>


</c74object>
